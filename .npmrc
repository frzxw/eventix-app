# NPM Configuration for Eventix

# Use exact versions
save-exact=true

# Increase timeout for slow connections
fetch-timeout=120000

# Registry
registry=https://registry.npmjs.org/

# Auth token (if needed for private packages)
# //registry.npmjs.org/:_authToken=YOUR_TOKEN

# Strict SSL (set to false only in development if needed)
strict-ssl=true

# Legacy peer deps if needed
legacy-peer-deps=false

# Audit
audit=true

# Fund
fund=false
